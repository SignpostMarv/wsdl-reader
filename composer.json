{
    "name": "goetas-webservices/wsdl-reader",
    "description": "Pure PHP WSDL parser",
    "keywords": ["xsd", "xml", "soap", "webservice", "wsdl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Asmir Mustafic",
            "email": "goetas@gmail.com"
        }
    ],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/SignpostMarv/xsd-reader.git"
		}
	],
    "require": {
        "php": ">=5.6",
        "goetas-webservices/xsd-reader": "dev-static-analysis",
        "symfony/event-dispatcher" : "^2.4||^3.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^4.8|^5.0",
        "jmikola/wildcard-event-dispatcher": "~1.0"
    },
    "autoload": {
        "psr-4": { "GoetasWebservices\\XML\\WSDLReader\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "GoetasWebservices\\XML\\WSDLReader\\Tests\\": "tests/" }
    },    
    "extra":{
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
